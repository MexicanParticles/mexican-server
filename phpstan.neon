parameters:
	level: 7
	paths:
		- app
		- src
	ignoreErrors:
		- '#Parameter \#[1-9] \$argument of class ReflectionClass constructor expects class-string\<T of object\>\|T of object, [a-zA-Z\\]+ given#'
		- '#Undefined variable: \$this#'
		- '#Using \$this outside a class#'
	reportUnmatchedIgnoredErrors: false
